  "bugs": {
    "url": "https://github.com/KonsumGandalf/AsteoFrontend/issues"
  },
  "homepage": "https://github.com/KonsumGandalf/AsteoFrontend#readme",
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
